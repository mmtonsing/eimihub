

{
  "rewrites": [
    {
      "source": "/auth/:match*",
      "destination": "/auth/:match*"
    },
    {
      "source": "/api/:match*",
      "destination": "/api/:match*"
    },
    {
      "source": "/blogs/:match*",
      "destination": "/blogs/:match*"
    },
    {
      "source": "/:path*",
      "destination": "/index.html"
    }
  ]
}
